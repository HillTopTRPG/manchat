<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<table id="api_v1_users" class='basic-table'>
  <thead>
  <tr>
    <th>id</th>
    <th>Uuid</th>
    <th>Name</th>
    <th>UserType</th>
    <th>Password</th>
    <th>RoomUuid</th>
    <th>Log in count</th>
    <th>Last logged in</th>
    <th>created_at</th>
    <th>updated_at</th>
    <th>Delete</th>
  </tr>
  </thead>
  <tbody>
  <% @api_v1_users.each do |api_v1_user| %>
    <%= render api_v1_user %>
  <% end %>
  </tbody>
</table>
