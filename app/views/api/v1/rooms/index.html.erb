<p style="color: green"><%= notice %></p>

<h1>Rooms</h1>

<table id="api_v1_rooms">
  <thead>
  <tr>
    <th>id</th>
    <th>Uuid</th>
    <th>Name</th>
    <th>Password</th>
    <th>Last logged in</th>
    <th>created_at</th>
    <th>updated_at</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
  </thead>
  <tbody>
  <% @api_v1_rooms.each do |api_v1_room| %>
    <%= render api_v1_room %>
  <% end %>
  </tbody>
</table>

<%= link_to "New room", new_api_v1_room_path %>
